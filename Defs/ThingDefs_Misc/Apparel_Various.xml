	<ThingDef ParentName="ApparelMakeableBase">
    <defName>Apparel_Witchesstockings</defName>
    <label>witches stockings</label>
    <description>A feminine, striped pair of stockings worn by witches and other sultry denizens of the night; spooky or otherwise.</description>
    <recipeMaker>
      <researchPrerequisite>ComplexClothing</researchPrerequisite>
    </recipeMaker>
    <graphicData>
      <texPath>Things/Pawn/Humanlike/Apparel/witchesstockings</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <costStuffCount>50</costStuffCount>
    <stuffCategories>
      <li>Fabric</li>
      <li>Leathery</li>
    </stuffCategories>
    <thingCategories>
      <li>Apparel</li>
    </thingCategories>
    <statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToMake>7000</WorkToMake>
      <Mass>0.5</Mass>
      <ArmorRating_Blunt>0.03</ArmorRating_Blunt>
      <ArmorRating_Sharp>0.03</ArmorRating_Sharp>
      <Insulation_Cold>-3</Insulation_Cold>
    </statBases>
	 <equippedStatOffsets>
      <MoveSpeed>0.05</MoveSpeed>
	  <SocialImpact>0.10</SocialImpact>
    </equippedStatOffsets>
    <apparel>
      <bodyPartGroups>
        <li>Legs</li>
      </bodyPartGroups>
      <layers>
        <li>OnSkin</li>
      </layers>
      <tags>
        <li>Outlander</li>
        <li>Spacer</li>
      </tags>
      <defaultOutfitTags>
        <li>Worker</li>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>
    <colorGenerator Class="ColorGenerator_StandardApparel" />
  </ThingDef>
  
  
